<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="screenGoVM" type="com.app.app.modules.screengo.data.viewmodel.ScreenGoVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearScreenGo"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:gravity="start"
            android:id="@+id/imageScreenshot2023"
            android:layout_height="@dimen/_369pxv"
            android:layout_marginTop="@dimen/_146pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_57pxh}"
            imageRes="@{@drawable/img_screenshot2023}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_screenshot2023"/>
        <TextView android:ellipsize="end" android:id="@+id/txtSharedGo"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_116pxh"
            android:layout_marginStart="@dimen/_116pxh"
            android:layout_marginTop="@dimen/_75pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{screenGoVM.screenGoModel.txtSharedGo}"
            style="@style/txt_2" tools:text="@string/lbl_shared_go"/>
        <FrameLayout android:id="@+id/frameStackarrowright"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_56pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_116pxh"
            android:layout_marginStart="@dimen/_116pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="@dimen/_71pxh" style="@style/groupStyleteal_600cornerRadius">
            <ImageView android:id="@+id/imageArrowright"
                android:layout_gravity="center"
                android:layout_height="@dimen/_27pxh"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_39pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowright"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
        </FrameLayout>
    </LinearLayout>
</layout>
